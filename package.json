{
  "name": "rolodex",
  "version": "0.3.3",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@getstation/electron-google-oauth2": "^1.1.2",
    "csv-parse": "^4.6.3",
    "elasticlunr": "^0.9.5",
    "electron-is-dev": "^1.1.0",
    "ews-javascript-api": "^0.9.6",
    "fs-extra": "^8.1.0",
    "googleapis": "^44.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.15",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "sqlite3": "^4.1.0",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "postinstall": "install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "icons": "./make-icons.sh icon.svg public",
    "clean": "rm -rf dist && rm -rf build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "iconutil": "^1.0.2"
  },
  "build": {
    "appId": "com.johnjonesfour.rolodex",
    "productName": "Rolodex",
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "publish": ["github"]
    },
    "win": {
      "publish": ["github"]
    },
    "linux": {
      "category": "Utility",
      "publish": ["github"]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
